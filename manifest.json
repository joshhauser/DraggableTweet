{
  "manifest_version": 2,
  "name": "TWDrag",
  "version": "1.0",

  "description": "Make new tweet popin draggable.",

  "icons": {
    "48": "icons/twitter-48.png",
    "96": "icons/twitter-96.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*", "https://twitter.com/*"],
      "js": ["script.js"]
    }
  ]
}